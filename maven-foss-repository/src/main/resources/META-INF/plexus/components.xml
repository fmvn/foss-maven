<component-set>
    <components>
        <!-- make sure FossRepositorySystem is listed first -->
        <component>
            <role>org.sonatype.aether.RepositorySystem</role>
            <role-hint></role-hint>
            <implementation>org.fedoraproject.maven.repository.internal.FossRepositorySystem</implementation>
            <!--
            <description></description>
            <isolated-realm>false</isolated-realm>
            -->
        </component>
        <component>
            <role>org.apache.maven.model.building.DefaultModelBuilder</role>
            <role-hint>default</role-hint>
            <implementation>org.apache.maven.model.building.DefaultModelBuilder</implementation>
            <description></description>
            <isolated-realm>false</isolated-realm>
            <requirements>
                <requirement>
                    <role>org.apache.maven.model.building.ModelProcessor</role>
                    <role-hint></role-hint>
                    <field-name>modelProcessor</field-name>
                </requirement>
                <requirement>
                    <role>org.apache.maven.model.validation.ModelValidator</role>
                    <role-hint>skip-effective</role-hint>
                    <field-name>modelValidator</field-name>
                </requirement>
                <requirement>
                    <role>org.apache.maven.model.normalization.ModelNormalizer</role>
                    <role-hint></role-hint>
                    <field-name>modelNormalizer</field-name>
                </requirement>
                <requirement>
                    <role>org.apache.maven.model.interpolation.ModelInterpolator</role>
                    <role-hint></role-hint>
                    <field-name>modelInterpolator</field-name>
                </requirement>
                <requirement>
                    <role>org.apache.maven.model.path.ModelPathTranslator</role>
                    <role-hint></role-hint>
                    <field-name>modelPathTranslator</field-name>
                </requirement>
                <requirement>
                    <role>org.apache.maven.model.path.ModelUrlNormalizer</role>
                    <role-hint></role-hint>
                    <field-name>modelUrlNormalizer</field-name>
                </requirement>
                <requirement>
                    <role>org.apache.maven.model.superpom.SuperPomProvider</role>
                    <role-hint></role-hint>
                    <field-name>superPomProvider</field-name>
                </requirement>
                <requirement>
                    <role>org.apache.maven.model.inheritance.InheritanceAssembler</role>
                    <role-hint></role-hint>
                    <field-name>inheritanceAssembler</field-name>
                </requirement>
                <requirement>
                    <role>org.apache.maven.model.profile.ProfileSelector</role>
                    <role-hint></role-hint>
                    <field-name>profileSelector</field-name>
                </requirement>
                <requirement>
                    <role>org.apache.maven.model.profile.ProfileInjector</role>
                    <role-hint></role-hint>
                    <field-name>profileInjector</field-name>
                </requirement>
                <requirement>
                    <role>org.apache.maven.model.management.PluginManagementInjector</role>
                    <role-hint></role-hint>
                    <field-name>pluginManagementInjector</field-name>
                </requirement>
                <requirement>
                    <role>org.apache.maven.model.management.DependencyManagementInjector</role>
                    <role-hint></role-hint>
                    <field-name>dependencyManagementInjector</field-name>
                </requirement>
                <requirement>
                    <role>org.apache.maven.model.composition.DependencyManagementImporter</role>
                    <role-hint></role-hint>
                    <field-name>dependencyManagementImporter</field-name>
                </requirement>
                <requirement>
                    <role>org.apache.maven.model.plugin.LifecycleBindingsInjector</role>
                    <role-hint></role-hint>
                    <field-name>lifecycleBindingsInjector</field-name>
                    <optional>true</optional>
                </requirement>
                <requirement>
                    <role>org.apache.maven.model.plugin.PluginConfigurationExpander</role>
                    <role-hint></role-hint>
                    <field-name>pluginConfigurationExpander</field-name>
                </requirement>
                <requirement>
                    <role>org.apache.maven.model.plugin.ReportConfigurationExpander</role>
                    <role-hint></role-hint>
                    <field-name>reportConfigurationExpander</field-name>
                </requirement>
                <requirement>
                    <role>org.apache.maven.model.plugin.ReportingConverter</role>
                    <role-hint></role-hint>
                    <field-name>reportingConverter</field-name>
                </requirement>
            </requirements>
        </component>
        <component>
            <role>org.sonatype.aether.impl.ArtifactDescriptorReader</role>
            <role-hint></role-hint>
            <implementation>org.apache.maven.repository.internal.DefaultArtifactDescriptorReader</implementation>
            <description></description>
            <isolated-realm>false</isolated-realm>
            <requirements>
                <requirement>
                    <role>org.sonatype.aether.spi.log.Logger</role>
                    <role-hint></role-hint>
                    <field-name>logger</field-name>
                </requirement>
                <requirement>
                    <role>org.sonatype.aether.impl.RemoteRepositoryManager</role>
                    <role-hint></role-hint>
                    <field-name>remoteRepositoryManager</field-name>
                </requirement>
                <requirement>
                    <role>org.sonatype.aether.impl.VersionResolver</role>
                    <role-hint></role-hint>
                    <field-name>versionResolver</field-name>
                </requirement>
                <!-- loop back to FOSS Repository -->
                <requirement>
                    <role>org.sonatype.aether.RepositorySystem</role>
                    <role-hint></role-hint>
                    <field-name>artifactResolver</field-name>
                </requirement>
                <requirement>
                    <role>org.apache.maven.model.building.ModelBuilder</role>
                    <role-hint></role-hint>
                    <field-name>modelBuilder</field-name>
                </requirement>
            </requirements>
        </component>
        <component>
            <role>org.sonatype.aether.impl.internal.DefaultRepositorySystem</role>
            <role-hint>default</role-hint>
            <implementation>org.sonatype.aether.impl.internal.DefaultRepositorySystem</implementation>
            <description></description>
            <isolated-realm>false</isolated-realm>
            <requirements>
                <requirement>
                    <role>org.sonatype.aether.spi.log.Logger</role>
                    <role-hint></role-hint>
                    <field-name>logger</field-name>
                </requirement>
                <requirement>
                    <role>org.sonatype.aether.impl.VersionResolver</role>
                    <role-hint></role-hint>
                    <field-name>versionResolver</field-name>
                </requirement>
                <requirement>
                    <role>org.sonatype.aether.impl.VersionRangeResolver</role>
                    <role-hint></role-hint>
                    <field-name>versionRangeResolver</field-name>
                </requirement>
                <!-- loop back to FOSS Repository -->
                <requirement>
                    <role>org.sonatype.aether.RepositorySystem</role>
                    <role-hint></role-hint>
                    <field-name>artifactResolver</field-name>
                </requirement>
                <requirement>
                    <role>org.sonatype.aether.impl.MetadataResolver</role>
                    <role-hint></role-hint>
                    <field-name>metadataResolver</field-name>
                </requirement>
                <requirement>
                    <role>org.sonatype.aether.impl.ArtifactDescriptorReader</role>
                    <role-hint></role-hint>
                    <field-name>artifactDescriptorReader</field-name>
                </requirement>
                <!-- loop back to FOSS Repository -->
                <requirement>
                    <role>org.sonatype.aether.RepositorySystem</role>
                    <role-hint></role-hint>
                    <field-name>dependencyCollector</field-name>
                </requirement>
                <requirement>
                    <role>org.sonatype.aether.impl.Installer</role>
                    <role-hint></role-hint>
                    <field-name>installer</field-name>
                </requirement>
                <requirement>
                    <role>org.sonatype.aether.impl.Deployer</role>
                    <role-hint></role-hint>
                    <field-name>deployer</field-name>
                </requirement>
                <requirement>
                    <role>org.sonatype.aether.spi.localrepo.LocalRepositoryManagerFactory</role>
                    <field-name>managerFactories</field-name>
                </requirement>
                <requirement>
                    <role>org.sonatype.aether.impl.SyncContextFactory</role>
                    <role-hint></role-hint>
                    <field-name>syncContextFactory</field-name>
                </requirement>
            </requirements>
        </component>
        <component>
            <role>org.sonatype.aether.impl.DependencyCollector</role>
            <role-hint>default</role-hint>
            <implementation>org.sonatype.aether.impl.internal.DefaultDependencyCollector</implementation>
            <description></description>
            <isolated-realm>false</isolated-realm>
            <requirements>
                <requirement>
                    <role>org.sonatype.aether.spi.log.Logger</role>
                    <role-hint></role-hint>
                    <field-name>logger</field-name>
                </requirement>
                <requirement>
                    <role>org.sonatype.aether.impl.RemoteRepositoryManager</role>
                    <role-hint></role-hint>
                    <field-name>remoteRepositoryManager</field-name>
                </requirement>
                <!-- loop back to FOSS Repository -->
                <requirement>
                    <role>org.sonatype.aether.RepositorySystem</role>
                    <role-hint></role-hint>
                    <field-name>descriptorReader</field-name>
                </requirement>
                <!-- loop back to FOSS Repository -->
                <requirement>
                    <role>org.sonatype.aether.RepositorySystem</role>
                    <role-hint></role-hint>
                    <field-name>versionRangeResolver</field-name>
                </requirement>
            </requirements>
        </component>
    </components>
</component-set>